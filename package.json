{
  "name": "@intuit/spring-cloud-config-inspector",
  "version": "0.7.5",
  "description": "The inspection tool for Spring Cloud Config repositories",
  "main": "./dist/intuit-spring-cloud-config-inspector.js",
  "scripts": {
    "test": "jest",
    "prebuild": "rm -rf build && mkdir build && cp -R public/* build/",
    "build": "NODE_ENV=production webpack --config ./webpack.config.js --progress --colors",
    "predeploy": "npm run build && echo Ready to use $npm_config_env",
    "deploy": "NODE_DEBUG=gh-pages gh-pages -d build --dest $npm_config_env/standalone --message \"Deploying to $npm_config_env environment\"",
    "prestart": "npm run prebuild && npm test",
    "start": "concurrently --kill-others --raw \"npm run proxy\" \"npm run dev\"",
    "dev": "webpack-dev-server --host dev.intuit.com --port 3232",
    "proxy": "node tools/cors-proxy"
  },
  "repository": {
    "type": "git",
    "url": "git@github.intuit.com:services-config/config-inspector.git"
  },
  "author": "Alexi Stein",
  "license": "ISC",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|txt)$": "<rootDir>/app/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/app/__mocks__/styleMock.js"
    }
  },
  "dependencies": {
    "babel-preset-stage-2": "^6.24.1",
    "json-loader": "^0.5.7",
    "prismjs": "^1.6.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-addons-css-transition-group": "^15.6.0",
    "react-prism": "^4.3.0",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.71.4",
    "url-search-params": "^0.10.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.17.2",
    "bootstrap": "^3.3.7",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.4",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "gh-pages": "^1.0.0",
    "jest": "^20.0.4",
    "jest-fetch-mock": "^1.2.1",
    "node-sass": "^4.5.3",
    "path": "^0.12.7",
    "properties-loader": "0.0.1",
    "raw-loader": "^0.5.1",
    "react-dom": "^15.6.1",
    "regenerator-runtime": "^0.10.5",
    "request": "^2.81.0",
    "sass-loader": "^6.0.6",
    "serve-favicon": "^2.4.3",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.4",
    "webpack-dev-server": "^2.7.1",
    "yaml-loader": "^0.5.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.intuit.net"
  },
  "tags": [
    "configuration",
    "config",
    "react",
    "inspection",
    "debug"
  ],
  "keywords": [
    "react",
    "react-component",
    "services-config",
    "config",
    "caas",
    "debug",
    "inspector",
    "inspection"
  ]
}
